<h1>공지사항 수정</h1>

<% form_for(@notice) do |f| %>
  <%= f.error_messages %>
  <input type="hidden" name="notice[temp_id]" value="<%= @notice.id.to_s %>">
<p>
    <%= f.label "공지여부" %><%= f.check_box :is_notice %>
  </p>
  
<p>
  <%= f.label :제목 %><br />
  <%= f.text_field :title %>
</p>	
<p>
  <%= f.label :내용 %><br />
  <%= f.text_area :content %>
</p>
 <p>
   <%= f.submit '수정' %><input type="button" value="취소" onClick="history.back(-1);">
 </p>

<% end %>
